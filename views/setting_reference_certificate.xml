<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="setting_reference_certificate" model="ir.actions.act_window">
            <field name="name">憑證種類維護</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">setting.reference.certificate</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="setting_reference_certificate_tree" model="ir.ui.view">
            <field name="name">憑證種類維護</field>
            <field name="model">setting.reference.certificate</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="certifikind"/>
                    <field name="certificont"/>
                </tree>
            </field>
        </record>
        <record id="setting_reference_certificate_form" model="ir.ui.view">
            <field name="name">憑證種類維護</field>
            <field name="model">setting.reference.certificate</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <h1>憑證種類維護</h1>
                        <group col="2">
                            <field name="certifikind"/>
                            <field name="certificont"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>