<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="setting_reference_taxoffical" model="ir.actions.act_window">
            <field name="name">國稅稽徵機關維護</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">setting.reference.taxoffical</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="setting_reference_taxoffical_tree" model="ir.ui.view">
            <field name="name">國稅稽徵機關維護</field>
            <field name="model">setting.reference.taxoffical</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="taxauthoritycode"/>
                    <field name="taxauthorityname"/>
                </tree>
            </field>
        </record>
        <record id="setting_reference_taxoffical_form" model="ir.ui.view">
        <field name="name">setting_reference_taxoffical_form</field>
        <field name="model">setting.reference.taxoffical</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1>國稅稽徵機關維護</h1>
                    <group>
                        <group col="4">
                            <field name="taxauthoritycode"/>
                            <field name="taxauthorityname"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    </data>
</openerp>